{
  "extends": ["react-app", "plugin:jsx-a11y/recommended"],
  "plugins": ["jsx-a11y", "react"],
  "rules" : {
     "indent": ["error", 2],
     "semi": [2, "always"],
     "quotes": ["error", "single"],
     "no-unused-vars": ["warn"],
     "react/jsx-uses-react": "error",
     "react/jsx-uses-vars": "error"
   }
}
